{
  "name": "@powerhousedao/project-management",
  "description": "The Scope of Work model defines a structured plan for executing contributor work; it includes deliverables and roadmaps with milestones with each being linked to ownership, progress tracking.",
  "version": "1.0.1",
  "license": "AGPL-3.0-only",
  "type": "module",
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./document-models": {
      "types": "./dist/document-models/index.d.ts",
      "default": "./dist/document-models/index.js"
    },
    "./editors": {
      "types": "./dist/editors/index.d.ts",
      "default": "./dist/editors/index.js"
    },
    "./subgraphs": {
      "types": "./dist/subgraphs/index.d.ts",
      "default": "./dist/subgraphs/index.js"
    },
    "./processors": {
      "types": "./dist/processors/index.d.ts",
      "default": "./dist/processors/index.js"
    },
    "./manifest": {
      "default": "./dist/powerhouse.manifest.json"
    },
    "./style.css": "./dist/style.css",
    "./document-models/*": {
      "types": "./dist/document-models/*/index.d.ts",
      "default": "./dist/document-models/*/index.js"
    },
    "./editors/*": {
      "types": "./dist/editors/*/index.d.ts",
      "default": "./dist/editors/*/index.js"
    }
  },
  "scripts": {
    "lint": "eslint --config eslint.config.js --cache --cache-strategy content",
    "lint:fix": "npm run lint -- --fix",
    "build:tsc": "tsc || true",
    "build:tsc:watch": "tsc --watch || true",
    "build:css": "npx @tailwindcss/cli -i ./style.css -o ./dist/style.css",
    "build:css:watch": "npx @tailwindcss/cli -i ./style.css -o ./dist/style.css --watch",
    "build": "npm run tsc && npm run tailwind",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "generate": "ph-cli generate",
    "connect": "ph-cli connect",
    "reactor": "ph-cli reactor",
    "service": "ph-cli service",
    "service-startup": "bash ./node_modules/@powerhousedao/ph-cli/dist/scripts/service-startup.sh",
    "service-unstartup": "bash ./node_modules/@powerhousedao/ph-cli/dist/scripts/service-unstartup.sh",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "tailwind": "bunx tailwindcss -i ./style.css -o ./dist/style.css",
    "check-circular-imports": "npx dpdm -T ./index.ts",
    "vetra": "ph-cli vetra",
    "migrate": "ph-cli migrate"
  },
  "dependencies": {
    "@powerhousedao/builder-profile": "1.0.2",
    "@powerhousedao/builder-tools": "5.3.2",
    "@powerhousedao/common": "5.3.2",
    "@powerhousedao/design-system": "5.3.2",
    "@powerhousedao/document-engineering": "1.40.1",
    "@powerhousedao/vetra": "5.3.2",
    "document-model": "5.3.2",
    "error": "^10.4.0",
    "graphql": "^16.10.0",
    "graphql-tag": "^2.12.6",
    "luxon": "^3.7.2",
    "uuid": "^11.1.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@electric-sql/pglite": "^0.2.17",
    "@eslint/js": "^9.37.0",
    "@openfeature/core": "^1.9.1",
    "@powerhousedao/analytics-engine-core": "^0.5.0",
    "@powerhousedao/codegen": "5.3.2",
    "@powerhousedao/config": "5.3.2",
    "@powerhousedao/connect": "5.3.2",
    "@powerhousedao/ph-cli": "5.3.2",
    "@powerhousedao/reactor-api": "5.3.2",
    "@powerhousedao/reactor-browser": "5.3.2",
    "@powerhousedao/reactor-local": "5.3.2",
    "@powerhousedao/scalars": "^2.0.1",
    "@powerhousedao/switchboard": "5.3.2",
    "@tailwindcss/cli": "^4.1.14",
    "@types/node": "^22.13.11",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.0.4",
    "document-drive": "5.3.2",
    "eslint": "^9.37.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "pm2": "^5.4.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "storybook": "^8.6.14",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0",
    "vite": "^6.2.3",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vitest": "^3.0.9",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "semantic-release": "^25.0.2"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "overrides": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "vite": "^6.2.3"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "staging",
        "prerelease": "staging"
      },
      {
        "name": "dev",
        "prerelease": "dev"
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "readme": "ERROR: No README data found!"
}
