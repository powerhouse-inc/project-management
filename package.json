{
  "name": "@powerhousedao/project-management",
  "description": "The Scope of Work model defines a structured plan for executing contributor work; it includes deliverables and roadmaps with milestones with each being linked to ownership, progress tracking.",
  "version": "1.0.0-dev.4",
  "license": "AGPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/powerhouse-inc/project-management"
  },
  "type": "module",
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "development": "./index.ts",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./document-models": {
      "development": "./document-models/index.ts",
      "types": "./dist/document-models/index.d.ts",
      "default": "./dist/document-models/index.js"
    },
    "./editors": {
      "development": "./editors/index.ts",
      "types": "./dist/editors/index.d.ts",
      "default": "./dist/editors/index.js"
    },
    "./subgraphs": {
      "development": "./subgraphs/index.ts",
      "types": "./dist/subgraphs/index.d.ts",
      "default": "./dist/subgraphs/index.js"
    },
    "./processors": {
      "development": "./processors/index.ts",
      "types": "./dist/processors/index.d.ts",
      "default": "./dist/processors/index.js"
    },
    "./manifest": {
      "development": "./powerhouse.manifest.json",
      "default": "./dist/powerhouse.manifest.json"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix --cache",
    "build:tsc": "tsc || true",
    "build:tsc:watch": "tsc --watch",
    "build:css": "npx @tailwindcss/cli -i ./style.css -o ./dist/style.css",
    "build:css:watch": "npx @tailwindcss/cli -i ./style.css -o ./dist/style.css --watch",
    "build": "npm run build:tsc && npm run build:css",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "generate": "ph-cli generate",
    "connect": "ph-cli connect",
    "reactor": "ph-cli reactor",
    "service": "ph-cli service",
    "service-startup": "bash ./node_modules/@powerhousedao/ph-cli/dist/scripts/service-startup.sh",
    "service-unstartup": "bash ./node_modules/@powerhousedao/ph-cli/dist/scripts/service-unstartup.sh"
  },
  "dependencies": {
    "@powerhousedao/builder-tools": "^5.3.4",
    "@powerhousedao/common": "^5.3.4",
    "@powerhousedao/design-system": "^5.3.4",
    "@powerhousedao/document-engineering": "^1.40.1",
    "document-model": "^5.3.4",
    "error": "^10.4.0",
    "graphql": "^16.10.0",
    "graphql-tag": "^2.12.6",
    "luxon": "^3.7.2",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@openfeature/core": "^1.9.1",
    "@electric-sql/pglite": "^0.2.17",
    "@eslint/js": "^9.37.0",
    "@powerhousedao/analytics-engine-core": "^0.6.4",
    "@powerhousedao/codegen": "^5.3.4",
    "@powerhousedao/config": "^5.3.4",
    "@powerhousedao/connect": "^5.3.4",
    "@powerhousedao/ph-cli": "^5.3.4",
    "@powerhousedao/reactor-api": "^5.3.4",
    "@powerhousedao/reactor-browser": "^5.3.4",
    "@powerhousedao/reactor-local": "^5.3.4",
    "@powerhousedao/scalars": "2.0.1",
    "@powerhousedao/switchboard": "^5.3.4",
    "@tailwindcss/cli": "^4.1.14",
    "@types/node": "^22.13.11",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.0.4",
    "document-drive": "^5.3.4",
    "eslint": "^9.37.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "pm2": "^5.4.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "storybook": "^8.6.14",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0",
    "vite": "^7.1.9",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vitest": "^3.0.9",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "semantic-release": "^25.0.2"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "overrides": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "staging",
        "prerelease": "staging"
      },
      {
        "name": "dev",
        "prerelease": "dev"
      }
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "chore",
              "release": "patch"
            },
            {
              "type": "ci",
              "release": "patch"
            },
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "style",
              "release": "patch"
            },
            {
              "type": "test",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  }
}
